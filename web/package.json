{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "google-map-react": "^1.1.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "NODE_ENV=development react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build-dev": "docker build -f ./docker/dev/Dockerfile -t $npm_package_name:dev .",
    "docker:dev": "docker run --rm --name $npm_package_name --net mybridge -it -p 3001:3000 -v `pwd`/src/:/usr/src/app/src/ $npm_package_name:dev",
    "docker:build-prod": "docker build -f ./docker/prod/Dockerfile -t gcr.io/project-33aa1bab-53bd-4/$npm_package_name:prod .",
    "docker:prod": "docker run --rm --name $npm_package_name --net mybridge -it -p 3002:80 gcr.io/project-33aa1bab-53bd-4/$npm_package_name:prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
